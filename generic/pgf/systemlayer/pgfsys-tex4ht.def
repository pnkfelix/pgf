% Driver commands for tex4ht

%
% Load common pdf commands:
%
\input pgfsys-common-svg.def

%
% pdftex-specific stuff:
%
\def\pgfsys@invoke#1{\HCode{#1}}

\def\pgfsys@beginpicture{\pgfsys@invoke{
<svg width="100pt" height="100pt" viewBox="0 0 100 100">
  <g transform="translate(0,100) scale(1,-1)" style="stroke:none;fill:none">}}
\def\pgfsys@endpicture{\pgfsys@invoke{</g></svg>}}

\endinput


